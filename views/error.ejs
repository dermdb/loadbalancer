<!DOCTYPE html>
<html lang="en">
<% include head.ejs %>
<body>
    <% if (page === 403){ %>
    <% include error/403.html%>
    <% } else if (page === 404) { %>
    <% include error/404.html%>
    <% } else if (page === 500) { %>
    <% include error/500.html%>
    <% } else if (page === 501) { %>
    <% include error/501.html%>
    <% } else if (page === 502) { %>
    <% include error/502.html%>
    <% } else if (page === 503) { %>
    <% include error/503.html%>
    <% } else if (page === 504) { %>
    <% include error/504.html%>
    <% } else if (page === "maintenance"){ %>
    <% include error/maintenance.html%>
    <% } else if (page === "limit"){ %>
    <% include error/rate-limit-error.html%>
    <% } %>
</body>
</html>